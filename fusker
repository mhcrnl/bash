echo -e "$DATE \n$URL \nRange: $NUM \nPrefixes: \n$PREFIX \n $LINE" >> $FILE

for PRE in $PREFIX
do
if [ "$4" != "" ]; then
  curl $URK/$URI/$PRE.jpg -f -o $MYPATH/#1#2#3.jpg --create-dirs
